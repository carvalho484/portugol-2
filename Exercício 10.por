programa {
  funcao inicio() {
    real base,altura
     escreva ("Qual e numero de (b): ")
    leia (base)
    escreva ("Qual e numero de (h): ")
    leia (altura)
    se (base == altura) {
      escreva ("E um quadrado")
    }senao {
      escreva ("E um retângulo")
    }
  }
}
// E um program capaz de ler dois numeros altura e base que representa um retângulo ou quadrado,
// e imprimir se ele é um quadrado ou um retângulo.
